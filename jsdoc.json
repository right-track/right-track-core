{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["modules/"],
    "exclude": ["node_modules/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "systemName": "Right Track Core",
    "systemSummary": "The core library for the Right Track projects",
    "systemColor": "#00897B",
    "footer": "Source Code for Right Track available at <a href='https://github.com/right-track'>GitHub/right-track</a>",
    "copyright": "Copyright &copy; 2021 David Waring and Right Track Contributors",
    "dateFormat": "YYYY-MM-DD",
    "collapseSymbols": false
  },
  "opts": {
    "readme": "README.md",
    "template": "./node_modules/foodoc/template",
    "destination": "doc/",
    "recurse": true
  }
}